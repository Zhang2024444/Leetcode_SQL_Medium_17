SELECT product_id,
new_price AS price 
FROM Products
WHERE (product_id,change_date) 
IN (SELECT product_id,MAX(change_date) AS change_date FROM products 
    WHERE change_date <='2019-08-16' GROUP BY product_id)
UNION
SELECT product_id, 10 as price 
From Products 
GROUP BY Product_id 
HAVING min(change_date)>'2019-08-16'
